<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr class="text-center">
                <th>No</th>
                <th>Date</th>
                <th>Title</th>
                <th>Name Bank</th>
                <th>Name Member</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tfoot>
            <tr class="text-center">
                <th>No</th>
                <th>Invoice</th>
                <th>Date</th>
                <th>Title</th>
                <th>Name Member</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </tfoot>
        <tbody>
            <% booking.map((data, index)=>{ %>
               <tr class="text-center"> 
                <td><%= index+1 %></td>
                <td><%= data.invoice %></td>
                <td><%= data.bookingStartDate.toLocaleString().split(',')[0] + ' s/d '+ data.bookingEndDate.toLocaleString().split(',')[0] %></td>
                <td><%= data.itemId.title %></td>
                <td><%= data.memberId.firstName +' '+data.memberId.lastName %></td>
                <td><%= data.payments.status %></td>
                <td>
                    <a href="/admin/booking/detail-booking/<%= data.id %>" class="btn btn-info btn-circle"><i class="fas fa-eye"></i></a>
                </td>
               </tr>
            <% }) %>
        </tbody>
    </table>
</div>
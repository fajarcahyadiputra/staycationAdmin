<div class="row">
      <div class="col-md-4">
            <a target="_blank" href="https://fajarcahyadiputra-nodejs.heroku.com/<%= booking.payments.proofPayment %>">
                <img width="320px" src="https://fajarcahyadiputra-nodejs.heroku.com/<%= booking.payments.proofPayment %>" alt="image of payment">
            </a>
          <% if(booking.payments.status == "process"){ %>
            <div class="d-flex justify-content-center mt-4">
                <a class="btn btn-primary" href="/admin/booking/update-status/<%= booking.id %>/accept"><i class="fas fa-check"></i></a>
                <a class="btn btn-warning ml-3" href="/admin/booking/update-status/<%= booking.id %>/reject"><i class="fas fa-times"></i></a>
            </div>
          <% } %>
      </div>
      <div class="col-md-8">
        <div class="form-group">
            <label>Invoice</label>
            <input type="text" readonly class="form-control" value="<%= booking.invoice %>">
        </div>
        <div class="form-group">
            <label>Title</label>
            <input type="text" readonly class="form-control" value="<%= booking.itemId.title %>">
        </div>
        <div class="form-group">
            <label>Price</label>
            <input type="text" readonly class="form-control" value="$ <%= booking.itemId.price %>">
        </div>
        <div class="form-group">
            <label>total</label>
            <input type="text" readonly class="form-control" value="$ <%= booking.total %>">
        </div>
        <div class="form-group">
            <label>From Bank</label>
            <input type="text" readonly class="form-control" value="<%= booking.payments.bankFrom %>">
        </div>
        <div class="form-group">
            <label>Status</label>
            <input type="text" readonly class="form-control" value="<%= booking.payments.status %>">
        </div>
      </div>
  </div>
